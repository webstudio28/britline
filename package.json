{
  "name": "raboteriya-landing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel css:watch serve",
    "dev": "npm start",
    "serve": "eleventy --serve",
    "build": "npm run css:build && eleventy",
    "css:build": "tailwindcss -i ./src/assets/css/tailwind.css -o ./src/assets/css/styles.css --minify",
    "css:watch": "tailwindcss -i ./src/assets/css/tailwind.css -o ./src/assets/css/styles.css --watch",
    "deploy": "echo 'Use GitHub Actions instead'",
    "postinstall": "node scripts/copy-pdfjs.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  }
}
